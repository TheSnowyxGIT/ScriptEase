{
  "cache": false,
  "check-coverage": false,
  "extension": [".ts"],
  "include": ["**/*.js", "**/*.ts"],
  "exclude": ["coverage/**", "dist/**", "node_modules/**", "**/*.d.ts", "**/*.test.ts", "**/*.spec.ts"],
  "sourceMap": true,
  "reporter": ["html", "text", "text-summary"],
  "all": true,
  "instrument": true
}
